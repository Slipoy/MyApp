{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
    "@react-native-community",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "semi": "error",
    "no-plusplus": "off"

  }
}
